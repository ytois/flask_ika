extends layout/frontend
block body
    h1= 'setting'
    div
      = get_flashed_messages()
    form(action='{{ url_for("SettingView:update") }}', method='post')
      input(type="hidden" name="csrf_token", value="{{ csrf_token() }}")
      input(type="text" name="iksm_session", value="{{ current_user.iksm_session }}")
      input(type="submit", value="送信")
